/* pages/party/partydetailpage/partydetailpage.wxss */
page{
  background-color: #1a237e;
}
.wrapper
{
  width: 100%;
  display: flex; 
  justify-content: center; 
  height: 100%;
  align-items:center;
  flex-direction: column;
}
.partymemberview{
  width: 670rpx; 
  background: #ffffff; 
  min-height: 100rpx;
  height: 200rpx; 
  box-sizing: border-box; 
  border:2px solid black;
  border-radius:0.5em;
  box-shadow: 5rpx 5rpx gray;
  display: flex;
  flex-direction: column;
  justify-content: center; 
  align-items:center;
  margin-top: 10px;
}
.partymemberlist{
  width: 100%; 
  height: 50%; 
  display: flex; 
  box-sizing: border-box; 
}
.partymemberlist .partymemberlistitem{
  height: 100%; 
  position: relative;
  display: flex;
  margin-left: 25rpx;
  justify-content: center; 
}
.partydetailview{
  width: 670rpx; 
  background: #ffffff; 
  min-height: 100rpx;
  height: 430rpx; 
  box-sizing: border-box; 
  border:2px solid black;
  border-radius:0.5em;
  box-shadow: 5rpx 5rpx gray;
  display: flex;
  flex-direction: column;
  justify-content: center; 
  align-items:center;
  margin-top: 30rpx;
}
.partymemberlist .partyMemberListitemlabel{
  font-size:15px;
  width: 78rpx;
   background: #e0e0e0;
    height: 32rpx; 
    display: block;
     box-sizing: border-box; 
     left: 9rpx; 
     position: absolute; 
     border-radius: 0.5em;
      top: 84rpx;
      text-align: center;
}
.partydetailcontent{
  width: 612rpx;
  background: #ffffff; 
  min-height: 100rpx; 
  height: 152rpx;
   display: block; 
   box-sizing: border-box;
   margin-top: 30rpx;
}
.partydetailtitle{
  margin-right: 480rpx;
  margin-bottom: 40rpx;
  font-weight: bold;
}
.joinbutton{
  width: 100px;
  height: 60rpx;
  margin-left: 400rpx;
  margin-bottom: -50rpx;
  background-color: #fbc02d;
  border-radius: 3rem;
 text-align: center;
 color: #ffffff;
 font-weight: bold;
 line-height:60rpx;
}